#include <iostream>
using namespace std;
#include <set>
   
int main()
{
   int arr1[4]={1,2,4,5};
   int arr2[4]={4,5,6,7};
   int n=4;
   set<int> st;
   for(int i=0;i<n;i++){
    st.insert(arr1[i]);
   }
   for(int i=0;i<n;i++){
    st.insert(arr2[i]);
   }
   cout<<"the array is\n";
   for(int i:st){
    cout<<i;
   }
   cout<<endl;
    return 0;
}
